#EXTM3U
#EXT-X-VERSION:3
#EXT-X-MEDIA-SEQUENCE:0
#EXT-X-ALLOWCACHE:1
#EXT-X-KEY:METHOD=AES-128,URI="{{ base_url }}{% url key track.pk %}",IV=0x{{ track.aes_iv }}

{% for segment in track.segments.select_related %}
